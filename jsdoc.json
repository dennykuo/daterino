{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(node_modules|docs)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/better-docs"
  },
  "plugins": []
}
